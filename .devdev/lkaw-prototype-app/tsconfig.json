{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "resolveJsonModule": true,
    "allowJs": true,
    "checkJs": true,
    "isolatedModules": true,
    "composite": true,
    "skipLibCheck": true,
    "moduleResolution": "bundler",
    "strict": true,
    "outDir": "./dist"
  },
  "extends": "@tsconfig/svelte/tsconfig.json",
  "include": ["ui/**/*.ts", "ui/**/*.js", "ui/**/*.svelte", "vite.config.ts"],
  "exclude": ["node_modules", "dist"]
}
